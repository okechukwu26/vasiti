{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "outDir": "dist",
      "baseUrl": ".",
      "sourceMap": false,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "pretty": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": ["ES6"],
      "noUnusedLocals": true
    },
    "include": ["src/**/*.ts", "src/api/User"],
    "exclude": ["node_modules"]
  }
  